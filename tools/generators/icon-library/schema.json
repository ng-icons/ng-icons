{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "icon-library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "x-prompt": "What should the library be called?",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "prefix": {
      "type": "string",
      "description": "The iconset prefix",
      "x-prompt": "The iconset prefix"
    },
    "license": {
      "type": "string",
      "description": "The iconset license",
      "x-prompt": "The iconset license"
    },
    "svgGlob": {
      "type": "string",
      "description": "Glob to the svg files name"
    }
  },
  "required": ["name", "prefix", "license"]
}
